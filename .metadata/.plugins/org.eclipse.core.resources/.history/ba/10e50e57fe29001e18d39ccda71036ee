	public static void main(String args[]) {
		try {
			Class c = Class.forName("Demo");
			System.out.println(c.isInterface());

			Class c2 = Class.forName("MyInterface");
			System.out.println(c2.isInterface());

		} catch (Exception e) {
			System.out.println(e);
		}
	}
}
